{
    "name": "willemo/laravel-twinfield",
    "description": "Twinfield service provider for Laravel",
    "type": "library",
    "license": "GNU GPLv3",
    "authors": [
        {
            "name": "Willem Oostendorp",
            "email": "willem@wioo.nl"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "^5.6@dev",
        "php-twinfield/twinfield": "dev-master",
        "symfony/options-resolver": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Willemo\\LaravelTwinfield\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
